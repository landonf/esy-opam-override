{
  "build": [
    [
      "bash",
      "-c",
      "#{os == 'windows' ? 'patch -p1 < dose3-5.0.1.patch' : 'true'}"
    ],
    [
      "./configure"
    ],
    [
      "#{(os == 'windows' || os == 'linux') ? 'make' : 'gmake'}",
      "printconf"
    ],
    [
      "bash",
      "-c",
      "#{os == 'windows' ? 'make' : (os == 'linux' ? 'make libs' : 'gmake libs')}"
    ]
  ]
}
